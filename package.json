{
    "name": "peepers",
    "version": "1.0.0",
    "description": "intelligent command line web scraper",
    "main": "index.js",
    "scripts": {
        "test": "BABEL_DISABLE_CACHE=1 && mocha --recursive --compilers js:babel-core/register",
        "test:watch": "BABEL_DISABLE_CACHE=1 && mocha --watch --compilers js:babel-core/register ./test ./",
        "build": "rm -rf ./dist && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log, --copy-files",
        "start": "yarn run build && node dist/peep.js"
    },
    "bin": {
        "peep": "yarn run build && node ./dist/peep.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/johnmaster208/peepers.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/johnmaster208/peepers/issues"
    },
    "homepage": "https://github.com/johnmaster208/peepers#readme",
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-register": "^6.26.0",
        "chalk": "^2.3.0",
        "cheerio": "^1.0.0-rc.2",
        "commander": "^2.14.1",
        "hoek": "^5.0.3",
        "keymirror": "^0.1.1",
        "node-emoji": "^1.8.1",
        "ora": "^2.0.0",
        "request": "^2.83.0",
        "request-promise": "^4.2.2"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "mocha": "^5.1.1",
        "nock": "^9.2.5",
        "sinon": "^5.0.2"
    }
}
